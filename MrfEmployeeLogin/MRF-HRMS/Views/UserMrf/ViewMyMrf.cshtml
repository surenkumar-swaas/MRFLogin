
@{
    ViewBag.Title = "ViewMyMrf";
}

<style>
    .content {
        width: 90%;
        margin-left: auto;
        margin-right: auto;
        height: 35px;
    }

    .btn-back {
        background-color: #42acd6;
        color: white;
        border: none;
        border-radius: 10px;
        padding: 10px;
        width: 68px;
    }
</style>

<div class="content">
      <button type="button" class="btn-back" id="back">Back</button>
</div>
<div class="Out-table" style="width:100%; height:400px; overflow:scroll; margin-top:30px">
    <table class="table table-bordered table-hover">
        <thead>
            <tr>
                <th>
                   ID
                </th>
                <th>
                    PositionName
                </th>
                 <th>
                    CreatedBy
                </th>
                
                <th>
                    VacancyFor
                </th>
                
                <th>
                    Territory
                </th>
                <th>
                    Edit/Delete
                </th>
               
            </tr>
        </thead>
        <tbody class="tbody"></tbody>
    </table>
</div>

<input type="hidden" id="getdata" value="@Session["UserID"]"/>

<script src="~/Scripts/jquery-3.3.1.min.js"></script>
<link href="~/Content/bootstrap.css" rel="stylesheet" />
<script src="~/Scripts/addscript.js"></script>





